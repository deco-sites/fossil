          }
        },
        {
          "value": {
            "page": {
              "__resolveType": "PLP Loader"
            },
            "layout": {
              "columns": {
                "mobile": 2,
                "desktop": 3
              },
              "variant": "aside"
            },
            "__resolveType": "site/sections/Product/SearchResult.tsx",
            "startingPage": 1
          },
          "rule": {
            "__resolveType": "website/matchers/always.ts"
          }
        }
      ]
    },
    {
      "__resolveType": "Footer - 01"
    },
    {
      "__resolveType": "YourViewsIntegration"
